{
  "dependencies": {
    "bootbox": "4.4.0",
    "bootstrap": "3.1.1",
    "guillotine": "1.3.1",
    "jquery": "1.12.4",
    "jquery-highlight": "3.4.0",
    "jquery-ui": "1.12.1",
    "moment": "2.21.0",
    "printthis": "0.1.5",
    "tinymce": "4.7.9"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "css-loader": "0.28.11",
    "eslint": "4.18.2",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-json": "1.2.0",
    "file-loader": "1.1.11",
    "imports-loader": "0.8.0",
    "lintspaces-cli": "0.6.0",
    "style-loader": "0.20.3",
    "stylelint": "9.1.1",
    "stylelint-config-standard": "18.2.0",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13",
    "webpack-merge": "4.1.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "ie >= 9"
            ]
          }
        }
      ]
    ]
  },
  "scripts": {
    "build:libs": "webpack --config webpack-libs.config.js",
    "build:dev": "webpack --config webpack-main-dev.config.js",
    "build:prod": "webpack --config webpack-main-prod.config.js",
    "build:test": "webpack --config webpack-test.config.js",
    "dev": "npm run build:libs && npm run build:dev && npm run build:test",
    "build": "npm run build:libs && npm run build:prod && npm run build:test",
    "lint:js": "eslint *.js src/main/webapp/dev -c static-analysis/teammates-eslint.yml",
    "lint:json": "eslint package.json src/main/webapp/data/*.json src/main/resources/*.json src/test/resources/data/*.json -c static-analysis/teammates-eslint-json.yml",
    "lint:css": "stylelint src/main/webapp/dev/css/*.css --config static-analysis/teammates-stylelint.yml",
    "lint:spaces": "lintspaces -n -t -d spaces -l 1 -. \"src/**/*.jsp\" \"src/**/*.tag\" \"src/main/**/*.html\" \"src/**/*.xml\" \"src/**/*.json\" \"src/main/webapp/dev/css/*.css\" \"src/**/*.java\" \"src/main/webapp/dev/**/*.js\" \"src/**/*.properties\" \"*.js\" \"*.json\" \"*.yml\" \"*.gradle\" \"static-analysis/*.*ml\"",
    "lint": "npm run lint:js && npm run lint:json && npm run lint:css && npm run lint:spaces"
  },
  "private": true
}
